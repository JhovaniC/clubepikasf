{% extends 'EpikaClubBundle::layout.html.twig' %}
{% block contenido_title %}Editar Afiliado{% endblock %}
{% block contenido %}

<form action="{{ path('afiliado_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
    {{ form_widget(edit_form) }}
    <p>
        <button type="submit">Edit</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('afiliado') }}">
            Back to the list
        </a>
    </li>
    <li>
        <form action="{{ path('afiliado_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}