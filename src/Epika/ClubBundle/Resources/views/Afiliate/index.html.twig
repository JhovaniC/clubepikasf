<h1>Afiliate list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Identification</th>
            <th>Name</th>
            <th>Last_name</th>
            <th>Address</th>
            <th>Neighborhood</th>
            <th>Phone</th>
            <th>Cellphone</th>
            <th>Email</th>
            <th>Gender</th>
            <th>Birth_date</th>
            <th>Created_at</th>
            <th>Updated_at</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('afiliado_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.identification }}</td>
            <td>{{ entity.name }}</td>
            <td>{{ entity.lastname }}</td>
            <td>{{ entity.address }}</td>
            <td>{{ entity.neighborhood }}</td>
            <td>{{ entity.phone }}</td>
            <td>{{ entity.cellphone }}</td>
            <td>{{ entity.email }}</td>
            <td>{{ entity.gender }}</td>
            <td>{{ entity.birthdate|date('Y-m-d H:i:s') }}</td>
            <td>{{ entity.createdat|date('Y-m-d H:i:s') }}</td>
            <td>{{ entity.updatedat|date('Y-m-d H:i:s') }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('afiliado_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('afiliado_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('afiliado_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
